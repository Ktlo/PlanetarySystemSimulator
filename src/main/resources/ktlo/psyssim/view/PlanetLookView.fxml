<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ColorPicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Line?>


<VBox alignment="TOP_CENTER" spacing="5.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label text="%viewName" />
      <ToggleButton mnemonicParsing="false" text="ToggleButton">
         <toggleGroup>
            <ToggleGroup fx:id="pictureType" />
         </toggleGroup>
      </ToggleButton>
      <ImageView fitHeight="150.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true" />
      <Line endX="100.0" startX="-100.0" />
      <ToggleButton mnemonicParsing="false" selected="true" text="ToggleButton" toggleGroup="$pictureType" />
      <ColorPicker editable="true" />
      <Line endX="100.0" startX="-100.0" />
      <ButtonBar>
        <buttons>
            <Button mnemonicParsing="false" text="Button" />
          <Button mnemonicParsing="false" text="Button" />
        </buttons>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets left="5.0" right="5.0" />
         </padding>
      </ButtonBar>
   </children>
</VBox>
